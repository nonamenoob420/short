//bubble sort
#include<stdio.h>
int main(){
    int a[10],i,j,n;
    printf("enter the range of array=");
    scanf("%d",&n);
    for(i=0;i<n;i++){
        printf("enter the elements=\n");
        scanf("%d",&a[i]);
    }
    for(i=0;i<n-1;i++){
        for(j=0;j<n-i-1;j++){
            if (a[j]>a[j+1]){
               int temp=a[j];
                a[j]=a[j+1];
                a[j+1]=temp;
            }
        }
    }
    printf("sorted array=\n");
    for(i=0;i<n;i++){
        printf("%d ,",a[i]);
    }
    return 0;
}
